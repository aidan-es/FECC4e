{
  "hosting": [
    {
      "site": "fecharactercreator",
    "public": "fecc_gui/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    ],
    "headers": [
      {
        "source": "/**/@(service-worker.js|index.html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/**!@(service-worker.js|index.html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
    },
    {
      "site": "fecharactercreator-analytics",
      "public": "fecharactercreator-analytics",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "umami-service",
            "region": "europe-west2"
          }
        }
      ]
    }
  ]
}
